{
  "expo": {
    "name": "myFridge",
    "slug": "myFridge",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/font/DMSans-Thin",
            "./assets/font/DMSans-Light",
            "./assets/font/DMSans-Regular",
            "./assets/font/DMSans-Meduim",
            "./assets/font/DMSans-SemiBold",
            "./assets/font/DMSans-Bold",
            "./assets/font/DMSans-Black",
            "./assets/font/DMSans-ExtraBold"
          ]
        }
      ]
    ]
  }
}
